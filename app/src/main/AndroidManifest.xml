<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pasiriihinen.golfstat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GolfStat"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pasiriihinen.golfstat.AddRoundActivity"></activity>
        <activity android:name="com.pasiriihinen.golfstat.AddNewCourseActivity" />
        <activity android:name="com.pasiriihinen.golfstat.ViewStatsActivity" />
        <activity android:name="com.pasiriihinen.golfstat.SelectCourseActivity" />
        <activity android:name="com.pasiriihinen.golfstat.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>